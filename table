import java.util.*;
public class table {
	public static void tab(int n) {
		for(int i=1;i<=10;i++) {
			int cal=n*i;
			System.out.println(n+" * "+i+" = "+cal);
		}
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number whose table needs to be written: ");
		int num=sc.nextInt();
		tab(num);

	}

}
