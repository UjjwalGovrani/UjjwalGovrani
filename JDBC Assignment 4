create database jdbcsession4;

use jdbcsession4;

CREATE TABLE Customer 
(
id INT AUTO_INCREMENT,
name VARCHAR(20),
email VARCHAR(20),
PRIMARY KEY (id)
);


INSERT INTO Customer (id,name,email) VALUES (101,'Jones','jones@gmail.com');
INSERT INTO Customer (id,name,email) VALUES (102,'Wills','wills@gmail.com');
INSERT INTO Customer (id,name,email) VALUES (103,'Smith','smith@gmail.com');
INSERT INTO Customer (id,name,email) VALUES (104,'Alex','alex@gmail.com');
INSERT INTO Customer (id,name,email) VALUES (105,'Anna','anna@gmail.com');

SELECT *FROM Customer;

SELECT *FROM Customer WHERE id=101;


UPDATE Customer SET name='Roy' where id=101;
SELECT *FROM Customer;

DELETE FROM Customer where id=104;
SELECT *FROM Customer;
